{
  "name": "Post",
  "plural": "Posts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "image": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "post_type_id": {
      "type": "number",
      "required": true
    },
    "user_id": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "postType": {
      "type": "belongsTo",
      "model": "PostType",
      "foreignKey": "post_type_id"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "user_id"
    }
  },
  "acls": [],
  "methods": {}
}
